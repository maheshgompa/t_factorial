          "rawSql": "WITH Recovered_mass_17 AS (\r\n    WITH A1 AS (\r\n    SELECT timestamp, value AS value_A1\r\n    FROM dewa_raw_measurement\r\n    WHERE sensor_id = 16 ---12WTU19CP001 \r\n),\r\nA2 AS (\r\n    SELECT timestamp, value AS value_A2\r\n    FROM dewa_raw_measurement\r\n    WHERE sensor_id = 17 ---12WTU17CP002\r\n),\r\nA3 AS (\r\n    SELECT timestamp, value AS value_A3\r\n    FROM dewa_raw_measurement\r\n    WHERE sensor_id = 11 ---12WTU10CT001\r\n),\r\nA4 AS (\r\n    SELECT timestamp, value AS value_A4\r\n    FROM dewa_raw_measurement\r\n    WHERE sensor_id = 15 ---12WBU17AP001XB01 \r\n    AND value = 1\r\n),\r\nDensity AS (\r\n    SELECT\r\n        timestamp,\r\n        (1065.05375654492 - 0.595426707667444 * A3.value_A3 - 0.000893338489101383 * (A3.value_A3 * A3.value_A3)) AS htf_density\r\n    FROM A3\r\n),\r\nH_values AS (\r\n    SELECT\r\n        A1.timestamp,\r\n        ((A1.value_A1 - A2.value_A2) * 10000) / D.htf_density AS H\r\n    FROM A1\r\n    INNER JOIN A2 ON A2.timestamp = A1.timestamp\r\n    INNER JOIN Density D ON D.timestamp = A1.timestamp\r\n),\r\nFlow AS (\r\n    SELECT\r\n        timestamp,\r\n        (-0.0001 * POWER(H, 4) + 0.0449 * POWER(H, 3) - 6.5986 * POWER(H, 2) + 430.08 * H - 10475) AS flow_value\r\n    FROM H_values\r\n),\r\nRecovered_mass AS (\r\n    SELECT\r\n        A4.timestamp,\r\n        F.flow_value * A4.value_A4 * D.htf_density * 10 / 3600 AS \"Recovered_mass_ton\"\r\n    FROM A4\r\n    INNER JOIN Flow F ON F.timestamp = A4.timestamp\r\n    INNER JOIN Density D ON D.timestamp = A4.timestamp\r\n)\r\nSELECT\r\n    TO_CHAR(timestamp::date, 'YYYY-MM-DD') AS date_1,\r\n    SUM(\"Recovered_mass_ton\") AS cumulative_recovered_mass_ton\r\nFROM\r\n    Recovered_mass\r\nGROUP BY\r\n    timestamp::date\r\nORDER BY\r\n    timestamp::date\r\n),\r\nRecovered_mass_18 AS (\r\nWITH B1 AS (\r\n    SELECT timestamp, value AS value_B1\r\n    FROM dewa_raw_measurement\r\n    WHERE sensor_id = 19 ---12WTU21CP001 \r\n),\r\nB2 AS (\r\n    SELECT timestamp, value AS value_B2\r\n    FROM dewa_raw_measurement\r\n    WHERE sensor_id = 20 ---12WTU18CP002\r\n),\r\nB3 AS (\r\n    SELECT timestamp, value AS value_B3\r\n    FROM dewa_raw_measurement\r\n    WHERE sensor_id = 11 ---12WTU10CT001\r\n),\r\nB4 AS (\r\n    SELECT timestamp, value AS value_B4\r\n    FROM dewa_raw_measurement\r\n    WHERE sensor_id = 18 ---12WBU17AP001XB01 \r\n    AND value = 1\r\n),\r\nDensity AS (\r\n    SELECT\r\n        timestamp,\r\n        (1065.05375654492 - 0.595426707667444 * B3.value_B3 - 0.000893338489101383 * (B3.value_B3 * B3.value_B3)) AS htf_density\r\n    FROM B3\r\n),\r\nH_values AS (\r\n    SELECT\r\n        B1.timestamp,\r\n        ((B1.value_B1 - B2.value_B2) * 10000) / D.htf_density AS H\r\n    FROM B1\r\n    INNER JOIN B2 ON B2.timestamp = B1.timestamp\r\n    INNER JOIN Density D ON D.timestamp = B1.timestamp\r\n),\r\nFlow AS (\r\n    SELECT\r\n        timestamp,\r\n        (-0.0001 * POWER(H, 4) + 0.0449 * POWER(H, 3) - 6.5986 * POWER(H, 2) + 430.08 * H - 10475) AS flow_value\r\n    FROM H_values\r\n),\r\nRecovered_mass AS (\r\n    SELECT\r\n        B4.timestamp,\r\n        F.flow_value * B4.value_B4 * D.htf_density * 10 / 3600 AS \"Recovered_mass_ton\"\r\n    FROM B4\r\n    INNER JOIN Flow F ON F.timestamp = B4.timestamp\r\n    INNER JOIN Density D ON D.timestamp = B4.timestamp\r\n)\r\nSELECT\r\n    TO_CHAR(timestamp::date, 'YYYY-MM-DD') AS date_1,\r\n    SUM(\"Recovered_mass_ton\") AS cumulative_recovered_mass_ton\r\nFROM\r\n    Recovered_mass\r\nGROUP BY\r\n    timestamp::date\r\nORDER BY\r\n    timestamp::date\r\n),\r\nCombinedRecoveredMass AS (\r\n    SELECT\r\n        COALESCE(RM17.date_1::date, RM18.date_1::date) AS date_1,\r\n        COALESCE(RM17.cumulative_recovered_mass_ton, 0) + COALESCE(RM18.cumulative_recovered_mass_ton, 0) AS combined_recovered_mass_ton\r\n    FROM Recovered_mass_17 RM17\r\n    FULL OUTER JOIN Recovered_mass_18 RM18 ON RM18.date_1 = RM17.date_1\r\n)\r\n\r\nSELECT\r\n    date_1::date as date_1,\r\n    combined_recovered_mass_ton\r\nFROM\r\n    CombinedRecoveredMass\r\nWHERE\r\n\r\n-- timestamp >= '$Date'::date - INTERVAL '60 days' -- Last 30 days filter\r\n--       AND timestamp <= '$Date'::date + INTERVAL '1 days'-- Reference date filter\r\n\r\n\r\n    date_1::date >= '$Date'::date - INTERVAL '60 days' -- Last 60 days filter\r\n    AND date_1::date <= '$Date'::date -- Reference date filter\r\nORDER BY\r\n    date_1::date;\r\n\r\n",
          "refId": "A",
