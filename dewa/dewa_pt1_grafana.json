          "rawQuery": true,
          "rawSql": "WITH ranked_high AS (\r\n    SELECT \r\n        id, \r\n        definition_valid_from, \r\n        definition_valid_to, \r\n        name, \r\n        unit, \r\n        value, \r\n        sensor_id, \r\n        order1, \r\n        is_high_alarm,\r\n        ROW_NUMBER() OVER (PARTITION BY sensor_id ORDER BY value DESC) AS rnk_high\r\n    FROM \r\n        dewa_alarm_definition\r\n    WHERE \r\n        name IN ('H', 'HH', 'HHH')\r\n),\r\nranked_low AS (\r\n    SELECT \r\n        id, \r\n        definition_valid_from, \r\n        definition_valid_to, \r\n        name, \r\n        unit, \r\n        value, \r\n        sensor_id, \r\n        order1, \r\n        is_high_alarm,\r\n        ROW_NUMBER() OVER (PARTITION BY sensor_id ORDER BY value ASC) AS rnk_low\r\n    FROM \r\n        dewa_alarm_definition\r\n    WHERE \r\n        name IN ('L', 'LL', 'LLL')\r\n),\r\ncombined_alarms AS (\r\n    SELECT \r\n        definition_valid_from, \r\n        name, \r\n        value, \r\n        sensor_id\r\n    FROM \r\n        ranked_high\r\n    WHERE \r\n        rnk_high = 1\r\n\r\n    UNION ALL\r\n\r\n    SELECT \r\n        definition_valid_from, \r\n        name, \r\n        value, \r\n        sensor_id\r\n    FROM \r\n        ranked_low\r\n    WHERE \r\n        rnk_low = 1\r\n)\r\nSELECT\r\n    DATE(t2.alarm_start) AS alarm_date,\r\n    COUNT(*) AS count_per_day\r\nFROM\r\n    dewa_alarm_record t2\r\nJOIN\r\n    combined_alarms t1 ON t2.alarm_definition_id = t1.sensor_id\r\nWHERE\r\n    (t1.name IN ('H', 'HH', 'HHH') AND t2.value > t1.value) OR\r\n    (t1.name IN ('L', 'LL', 'LLL') AND t2.value < t1.value)\r\nGROUP BY\r\n    DATE(t2.alarm_start)\r\nORDER BY\r\n    alarm_date;\r\n\r\n\r\n\r\n-- (t2.alarm_start = ${Date:singlequote}::timestamp)\r\n    ",
