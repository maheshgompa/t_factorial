          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH on_off_times AS (\r\n    SELECT\r\n        timestamp,\r\n        operation,\r\n        LAG(timestamp) OVER (ORDER BY timestamp) AS prev_timestamp,\r\n        LAG(operation) OVER (ORDER BY timestamp) AS prev_operation,\r\n        DATE_TRUNC('day', timestamp) AS day_timestamp -- Truncate timestamp to day level\r\n    FROM dewa_operation\r\n    WHERE operation IN ('ON', 'OFF') -- Assuming 'ON' and 'OFF' are valid operation values\r\n\r\n)\r\nSELECT\r\n     TO_CHAR(day_timestamp::date, 'DD-MM-YYYY') AS day, -- Format date as '01-02-2024'\r\n\tsum(case when TO_CHAR(timestamp::date, 'DD-MM-YYYY') =TO_CHAR(prev_timestamp::date, 'DD-MM-YYYY') then (EXTRACT(EPOCH FROM (timestamp - prev_timestamp)))/3600\r\n\telse 0\r\n\tend) as duration\r\n\t\r\n--     (EXTRACT(EPOCH FROM (timestamp - prev_timestamp)))/3600 AS duration -- Convert seconds to hours\r\nFROM on_off_times\r\nWHERE operation = 'ON'  -- Considering only 'ON' operation values\r\n-- and ((EXTRACT(EPOCH FROM (timestamp - prev_timestamp)))/3600)>1\r\n      AND timestamp >= '$Date'::date - INTERVAL '30 days' -- Last 30 days filter\r\n      AND timestamp <= '$Date'::date + INTERVAL '1 days'-- Reference date filter\r\nGROUP BY day_timestamp::date\r\nORDER BY day_timestamp::date;",
          "refId": "A",
          "sql": {
            "columns": [
