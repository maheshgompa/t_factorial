          "rawSql": "WITH formatted_dates AS (\r\n    SELECT\r\n        DATE(alarm_start) AS alarm_date,\r\n        COUNT(*) AS total_alarms\r\n        -- SUM(CASE WHEN dad.name IN ('HH', 'LL') THEN 1 ELSE 0 END) AS critical_alarms\r\n    FROM\r\n        dewa_alarm_record dar\r\n    JOIN\r\n        dewa_alarm_definition dad ON dar.alarm_definition_id = dad.id\r\n    GROUP BY\r\n        DATE(alarm_start)\r\n)\r\nSELECT\r\n    TO_CHAR(alarm_date, 'YYYY/MM/DD') AS alarm_date,\r\n    total_alarms\r\nFROM\r\n    formatted_dates\r\nWHERE\r\n    alarm_date = ${Date:singlequote}::timestamp \r\nORDER BY\r\n    alarm_date;\r\n",
