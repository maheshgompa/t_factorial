          "rawQuery": true,
          "rawSql": "SELECT\r\n    o.timestamp,\r\n    o.\"12wta07aa201\",\r\n    o.\"12wta07aa202\",\r\n    o.\"12wta07aa203\",\r\n    o.\"12wta07aa204\",\r\n    r.value AS \"12wtu10cl903\"\r\nFROM\r\n    (\r\n        SELECT\r\n            \"timestamp\",\r\n            \"12wta07aa201\",\r\n            \"12wta07aa202\",\r\n            \"12wta07aa203\",\r\n            \"12wta07aa204\"\r\n        FROM\r\n            dewa_operation\r\n    ) AS o\r\nLEFT JOIN\r\n    (\r\n        SELECT\r\n            \"timestamp\",\r\n            value\r\n        FROM\r\n            dewa_raw_measurement\r\n        WHERE\r\n            sensor_id = 2\r\n    ) AS r\r\nON o.timestamp = r.\"timestamp\"\r\nWHERE\r\n    o.timestamp between ${Date:singlequote}::timestamp and ${Date:singlequote}::timestamp + ${Duration::raw}\r\nORDER BY\r\n    o.\"timestamp\";",
